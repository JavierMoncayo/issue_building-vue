{
  "name": "base_front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "npm run lint -- --fix && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "deploy": "npm run build && cd docs && npm run build && cd .. && sudo gh-pages -d dist",
    "s_server_install": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "install:clean": "npm run s_server_install && npm run dev",
    "s_server_build": "npm run s_server_install && npm run build",
    
    "s_TO_PRODUCTION": "npm run _s_build && npm run _s_build_zip && npm run _s_remote_clean_previous && npm run _s_zip_to_remote && npm run _s_zip_unpack",
    
    "_s_build": "npm run build",
    "_s_build_zip": "zip -r $npm_package_config_name.zip dist",
    "_s_remote_clean_previous": "ssh -t $npm_package_config_ssh_user@$npm_package_config_ssh_host $npm_package_config_cmd_clean_previous",
    "_s_zip_to_remote": "scp $npm_package_config_name.zip $npm_package_config_ssh_user@$npm_package_config_ssh_host:$npm_package_config_remote_location",
    "_s_zip_unpack": "ssh -t $npm_package_config_ssh_user@$npm_package_config_ssh_host $npm_package_config_cmd_zip_unpack;"
  },

  "config" : {
    "ssh_host": "vvv.suments.com",
    "ssh_user": "pofenas",
    "remote_location": "/var/www/html/base_front.suments.com/",
    "name": "base_front", 
    "cmd_clean_previous": "rm -r /var/www/html/base_front.suments.com/*; exit",
    "cmd_zip_unpack": "cd /var/www/html/base_front.suments.com; unzip base_front; mv dist/* ./; exit"
  },

  "dependencies": {
    "@tweenjs/tween.js": "18.5.0",
    "@vue-stripe/vue-stripe": "^4.2.5",
    "@vue/shared": "^3.1.0",
    "axios": "^0.21.4",
    "chartist": "0.11.4",
    "es6-promise": "4.2.8",
    "fuse.js": "^6.4.6",
    "google-maps": "4.2.3",
    "jquery": "^3.6.0",
    "jvectormap-next": "3.1.0",
    "md5": "^2.3.0",
    "moment": "2.24.0",
    "nouislider": "14.1.1",
    "perfect-scrollbar": "1.5.0",
    "sweetalert2": "9.10.0",
    "vanilla-cookieconsent": "^2.5.0",
    "vee-validate": "3.2.5",
    "vue": "2.6.11",
    "vue-clickaway": "2.2.2",
    "vue-clipboard2": "^0.3.3",
    "vue-fuse": "^4.0.3",
    "vue-google-signin-button": "^1.0.4",
    "vue-gtag": "^1.16.1",
    "vue-i18n": "^8.24.4",
    "vue-loading-overlay": "^3.4.2",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "3.1.6",
    "vue2-touch-events": "^3.2.2",
    "vue2-transitions": "0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.2.3",
    "@vue/cli-plugin-eslint": "^4.5.14",
    "@vue/cli-plugin-router": "4.2.3",
    "@vue/cli-service": "4.2.3",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.2.2",
    "fibers": "4.0.2",
    "js-md5": "^0.7.3",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "vue-template-compiler": "2.6.11"
  }
}
